[user]
	name = ono ono
	email = azazabc123@gmail.com
[credential]
	helper = cache --timeout=3600
[color]
	ui = auto
[core]
	quotepath = off
[i18n]
	logoutputencoding = utf8
	commitencoding = utf8
[push]
	default = current

[url "git@github.com:"]
    insteadOf = https://github.com/
[cola]
	spellcheck = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[includeIf "gitdir:~/Intrising/"]
    path = ~/Intrising/.company_gitconfig
