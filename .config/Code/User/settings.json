{
    "editor.fontSize": 12,
    "editor.fontFamily": " 'DejaVu Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    },
    "tslint.autoFixOnSave": true,
    "breadcrumbs.enabled": true,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": " 'DejaVu Sans Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "window.zoomLevel": -1,
    "typescript.format.insertSpaceAfterSemicolonInForStatements": false,
    "typescript.locale": "en",
    "bracketPairColorizer.forceUniqueOpeningColor": true,
    "bracket-pair-colorizer-2.highlightActiveScope": true,
    "highlight.regexes": {
        "(/?/? ?(?:TODO|REFACTORING|OPTIMIZE): )(.*)": [
          {
            "overviewRulerColor": "#ffcc00",
            "backgroundColor": "#ffcc00",
            "color": "#1f1f1f",
            "fontWeight": "bold"
          },
          {
            "backgroundColor": "#d9ad00",
            "color": "#1f1f1f"
          }
        ],
        "(/?/? ?(?:NOTE|DISCUSS): )(.*)": [
          {
            "overviewRulerColor": "#00ccff",
            "backgroundColor": "#00ccff",
            "color": "#1f1f1f",
            "fontWeight": "bold"
          },
          {
            "backgroundColor": "#55ddff",
            "color": "#1f1f1f"
          }
        ],
        "(/?/? ?FIXME: )(.*)": [
          {
            "overviewRulerColor": "#ff0000",
            "backgroundColor": "#ff0000",
            "color": "#1f1f1f",
            "fontWeight": "bold"
          },
          {
            "backgroundColor": "#d90000",
            "color": "#1f1f1f"
          }
        ],
        "(/?/? ?DEPRECATED: )(.*)": [
          {
            "overviewRulerColor": "#ff6600",
            "backgroundColor": "#ff6600",
            "color": "#1f1f1f",
            "fontWeight": "bold"
          },
          {
            "backgroundColor": "#d95500",
            "color": "#1f1f1f"
          }
        ],
        "(/?/? ?ATTENTION: )(.*)": [
          {
            "overviewRulerColor": "#ff5555",
            "backgroundColor": "#ff5555",
            "color": "#1f1f1f",
            "fontWeight": "bold"
          },
          {
            "backgroundColor": "#aa4444",
            "color": "#1f1f1f"
          }
        ],
        "(// )(@\\w+)": [
          {},
          {
            "color": "#4de0ff"
          }
        ]
      },
      "search.collapseResults": "alwaysExpand",
      "search.smartCase": true,
      "colorHelper.disableGpu": 1,
      "workbench.iconTheme": "material-icon-theme",
      "todo.embedded.providers.ag.regex": "(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|DEPRECATED|DISCUSS|SPEC)",
      "todo.embedded.providers.rg.regex": "(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|DEPRECATED|DISCUSS|SPEC)",
      "todo.embedded.regex": "(?:<!-- *)?(?:#|//|/\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(TODO|FIXME|FIX|BUG|UGLY|HACK|NOTE|IDEA|REVIEW|DEBUG|OPTIMIZE|DEPRECATED|DISCUSS|SPEC)(?:\\s*\\([^)]+\\))?:?(?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|((?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))",
      "todo.colors.types": {
        "TODO": "#ffcc00",
        "FIXME": "#ff0000",
        "FIX": "#cc0000",
        "BUG": "#ff0000",
        "UGLY": "#cc0000",
        "DEBUG": "#cc0000",
        "HACK": "#cc0000",
        "ATTENTION": "#ff6600",
        "DEPRECATED": "#ff6600",
        "DISCUSS": "#ffcc00",
        "REVIEW": "#00ccff",
        "OPTIMIZE": "#00ccff",
        "TSC": "#00ccff",
        "NOTE": "#cc00cc",
        "IDEA": "#cc00cc",
      },
      "emmet.showSuggestionsAsSnippets": true,
      "workbench.editor.tabSizing": "shrink",
      "workbench.editor.enablePreview": false,
      "gitlens.codeLens.enabled": false,
      "workbench.editor.tabCloseButton": "off",
      "cSpell.diagnosticLevel": "Hint",
      "color-highlight.markRuler": false,
      "puglint.enable": true,
      "highlightwords.defaultMode": {
        "default": 1
      },
      "window.titleBarStyle": "custom",
      "C_Cpp.default.cppStandard": "c++17",
      "vetur.completion.tagCasing": "initial",
      "vetur.format.defaultFormatter.ts": "vscode-typescript",
      "files.trimTrailingWhitespace": true,
      "go.gotoSymbol.includeImports": true,
      "go.lintFlags": [
        "\"--exclude=\\\"\\bexported \\\\w+ (\\\\S*['.]*)([a-zA-Z'.*]*) should have comment or be unexported\\b\\\"\""
      ],
      "cSpell.enableFiletypes": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "python",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "vue",
        "yaml",
        "yml"
      ],
      "editor.formatOnType": true,
      "workbench.colorTheme": "One Monokai",
      "files.exclude": {
        "/lib/": true
      },
      "editor.suggestSelection": "first",
      "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
      "typescript.updateImportsOnFileMove.enabled": "always",
      "search.exclude": {
        "**/*.min.js": true,
        "**/*.pb*": true,
        "**/apidoc*.swagger.json": true,
        "/server/": true,
        "client/helps": true,
        "dist/": true,
        "package-lock.json": true,
        "third_party": true
      },
      "workbench.colorCustomizations": {},
      "outline.showArrays": false,
      "outline.showBooleans": false,
      "outline.showConstants": false,
      "outline.showConstructors": false,
      "outline.showEnumMembers": false,
      "outline.showEnums": false,
      "outline.showEvents": false,
      "outline.showInterfaces": false,
      "outline.showFiles": false,
      "outline.showStrings": false,
      "outline.showStructs": false,
      "outline.showObjects": false,
      "outline.showOperators": false,
      "outline.showPackages": false,
      "outline.showVariables": false,
      "outline.showTypeParameters": false,
      "outline.showNull": false,
      "outline.showNumbers": false,
      "javascript.updateImportsOnFileMove.enabled": "always",
      "go.useLanguageServer": true,
      "editor.colorDecorators": false,
      "editor.renderControlCharacters": true,
      "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
      ],
      "explorer.confirmDragAndDrop": false,
      "bracket-pair-colorizer-2.colors": [

        "Gold",
        "Orchid",
        "LightSkyBlue"
      ],
      "puglint.config": {

      }
    }
